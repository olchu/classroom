generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model student {
  id         Int      @id @default(autoincrement())
  firstName  String   @db.VarChar(255)
  secondName String   @db.VarChar(255)
  dob        DateTime @db.Date
}
