generator client {
  provider        = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model student {
  id          Int      @id @default(autoincrement())
  first_name  String   @db.VarChar(255)
  second_name String   @db.VarChar(255)
  dob         DateTime @db.Date
}
